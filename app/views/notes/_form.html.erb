<form action="/tracks/<%= params[:track_id] %>/notes" method="post">

  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

<textarea name="note[note_body]"
          rows="8"
          cols="40"
          value= "<%= @note.note_body %>">
        </textarea>

<br>

<input type="submit" value="Leave Note">


</form>
